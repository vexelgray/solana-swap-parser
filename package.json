{
  "name": "solana-swap-parser",
  "version": "1.0.3",
  "description": "A TypeScript library for parsing Solana DEX swap transaction results",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "README_zh.md",
    "LICENSE"
  ],
  "keywords": [
    "solana",
    "blockchain",
    "dex",
    "swap",
    "parser",
    "raydium",
    "orca",
    "jupiter"
  ],
  "author": "Your Name",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ideacco/solana-swap-parser.git"
  },
  "bugs": {
    "url": "https://github.com/ideacco/solana-swap-parser/issues"
  },
  "homepage": "https://github.com/ideacco/solana-swap-parser#readme",
  "scripts": {
    "build": "tsc",
    "prepare": "yarn build",
    "test": "jest --config jest.config.js --verbose",
    "test:build": "yarn build && yarn test",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "check-types": "tsc --noEmit"
  },
  "dependencies": {
    "@solana/buffer-layout": "^4.0.1",
    "@solana/buffer-layout-utils": "^0.2.0",
